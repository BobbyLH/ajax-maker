{
  "name": "ajax-maker",
  "version": "1.2.4",
  "description": "ajax request library base on axios",
  "main": "dist/index.js",
  "scripts": {
    "dev": "webpack-dashboard -p 6301 -- node demo/server.js",
    "test": "mocha --opts mocha.opts",
