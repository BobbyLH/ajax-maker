    "@babel/preset-env": "^7.23.7",
    "@types/chai": "^4.2.1",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "~6.17.0",
    "@typescript-eslint/parser": "~6.17.0",
    "axios": "^1.7.2",
    "babel-loader": "~9.1.3",
    "body-parser": "^1.20.0",
    "chai": "^4.2.0",
    "eslint": "^6.3.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.6.0",
    "mocha": "^6.2.0",
    "ts-loader": "~9.5.1",
    "ts-node": "~10.9.2",
    "typescript": "~5.3.3",
    "webpack": "^5.89.0",
